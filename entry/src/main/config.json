        "description": "$string:mainability_description",
        "label": "$string:entry_MainAbility",
        "type": "page",
        "launchType": "standard"
      },
      {
        "orientation": "portrait",
        "name": "com.kuili.takephoto.ImageAbility",
        "icon": "$media:icon",
        "description": "$string:imageability_description",
        "label": "$string:entry_MainAbility",
        "type": "page",
        "launchType": "standard"
      }
    ]
